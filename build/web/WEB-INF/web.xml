<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>OwnerLoginControler</servlet-name>
        <servlet-class>controler.owner.OwnerLoginControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchControler</servlet-name>
        <servlet-class>controler.bill.SearchControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertBillControler</servlet-name>
        <servlet-class>controler.bill.InsertControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBillControler</servlet-name>
        <servlet-class>controler.bill.EditControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBillControler</servlet-name>
        <servlet-class>controler.bill.DeleteControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchWorkerControler</servlet-name>
        <servlet-class>controler.worker.SearchControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertWorkerControler</servlet-name>
        <servlet-class>controler.worker.InsertControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditWorkerControler</servlet-name>
        <servlet-class>controler.worker.EditControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteWorkerControler</servlet-name>
        <servlet-class>controler.worker.DeleteControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchProductControler</servlet-name>
        <servlet-class>controler.product.SearchControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertProductControler</servlet-name>
        <servlet-class>controler.product.InsertControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProductControler</servlet-name>
        <servlet-class>controler.product.EditControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProductControler</servlet-name>
        <servlet-class>controler.product.DeleteControler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OwnerLoginControler</servlet-name>
        <url-pattern>/owner/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchControler</servlet-name>
        <url-pattern>/bill/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertBillControler</servlet-name>
        <url-pattern>/bill/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBillControler</servlet-name>
        <url-pattern>/bill/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBillControler</servlet-name>
        <url-pattern>/bill/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchWorkerControler</servlet-name>
        <url-pattern>/worker/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertWorkerControler</servlet-name>
        <url-pattern>/worker/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditWorkerControler</servlet-name>
        <url-pattern>/worker/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteWorkerControler</servlet-name>
        <url-pattern>/worker/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchProductControler</servlet-name>
        <url-pattern>/product/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertProductControler</servlet-name>
        <url-pattern>/product/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProductControler</servlet-name>
        <url-pattern>/product/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProductControler</servlet-name>
        <url-pattern>/product/delete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
